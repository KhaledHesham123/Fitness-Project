{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "IdentityServiceRoute": {
        "ClusterId": "IdentityServiceRouteCluster",
        "Match": {
          "Path": "/Identity/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/Identity"
          }
        ]
      },
      "NotificationServiceRoute": {
        "ClusterId": "NotificationServiceCluster",
        "Match": {
          "Path": "/Notification/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/Notification"
          }
        ],
        "RateLimiterPolicy": "logainPolicy"
      },
      "NutritionServiceRoute": {
        "ClusterId": "NutritionServiceCluster",
        "Match": {
          "Path": "/Nutrition/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/Nutrition"
          }
        ]
      },
      "ReportsServiceRoute": {
        "ClusterId": "ReportsServiceCluster",
        "Match": {
          "Path": "/Reports/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/Reports"
          }
        ]
      },
      "UserProfileServiceRoute": {
        "ClusterId": "UserProfileServiceCluster",
        "Match": {
          "Path": "/UserProfile/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/UserProfile"
          }
        ]
      },
      "UserTrainingTrackingServiceRoute": {
        "ClusterId": "UserTrainingTrackingServiceCluster",
        "Match": {
          "Path": "/UserTrainingTracking/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/UserTrainingTracking"
          }
        ]
      },
      "WorkoutCatalogServiceRoute": {
        "ClusterId": "WorkoutCatalogServiceCluster",
        "Match": {
          "Path": "/WorkoutCatalog/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/WorkoutCatalog"
          }
        ],
        "RateLimiterPolicy": "DefaultServicePolicy"
      }
    },
    "Clusters": {
      "IdentityServiceRouteCluster": {
        "Destinations": {
          "Identity": {
            "Address": "https://identityservice:8081"
          }
        }
      },
      "NotificationServiceCluster": {
        "Destinations": {
          "Notification": {
            "Address": "https://notificationservice:8081"
          }
        }
      },
      "NutritionServiceCluster": {
        "Destinations": {
          "Nutrition": {
            "Address": "https://nutritionservice:8081"
          }
        }
      },
      "ReportsServiceCluster": {
        "Destinations": {
          "Reports": {
            "Address": "https://reportsservice:8081"
          }
        }
      },
      "UserProfileServiceCluster": {
        "Destinations": {
          "UserProfile": {
            "Address": "https://userprofileservice:8081"
          }
        }
      },
      "UserTrainingTrackingServiceCluster": {
        "Destinations": {
          "UserTrainingTracking": {
            "Address": "https://usertrainingtrackingservice:8081"
          }
        }
      },
      "WorkoutCatalogServiceCluster": {
        "Destinations": {
          "WorkoutCatalog": {
            "Address": "https://workoutcatalogservice:8081"
          }
        }
      }
    }
  }
}
